{
  "manifest_version": 3,
  "name": "YouTube History",
  "version": "1.0",
  "description": "Shows you the first two videos from YouTube History",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-64.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "history"
  ],
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
